// giai pt tuyen tinh bac nhat 2 an
#include <stdio.h>

int main(){
	float a,b,c,d,e,f ;
	printf("nhap a,b,c,d,e,f :"); scanf("%f %f %f %f %f %f",&a,&b,&c,&d,&e,&f);
	float D= a*e - d*b , Dx= c*e-b*f ,Dy=a*f-c*d;
	if (D==0){
		if (Dx=Dy==0){
			printf("he pt co vo so nghiem");
		}
		else{
			printf("he pt vo nghiem");
		}
	}
	else{
		float x=Dx/D ,y=Dy/D;
		printf("he pt co nghiem duy nhat (x,y)=(%.2f,%.2f)",x,y);
	}
	return 0;
 }
